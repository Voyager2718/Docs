package trie;

import java.util.ArrayList;
import java.util.List;

public class Trie {
	private int SIZE;
	private Trie rootNode;

	public Trie(char value) {
		this.rootNode = new Trie(value);
	}

	public void addNode(Trie node) {
		this.l.add(node);
	}

	public char getValue(Trie node) {
		return node.value;
	}
}
