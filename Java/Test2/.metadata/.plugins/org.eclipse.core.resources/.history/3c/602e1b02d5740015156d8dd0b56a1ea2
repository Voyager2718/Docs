package test;

abstract class Father {
	public int getA() {
		return 0;
	}

	public Father() {
		super();
	}
}

class Son extends Father {
	private int a;

	public Son() {
		a = 20;
	}

	public int getA() {
		return a;
	}
}

public class Test {
	public static void main(String args[]) {
		Son son = new Son();
		System.out.println((new Son()).hashCode());
		System.out.println((new Son()).hashCode());
		System.out.println(son.hashCode());
		System.out.println(son.hashCode());
	}
}
