package test;

class Father {
	protected int a;

	public Father() {
		a = 10;
	}

	public int getA() {
		return a;
	}
}

class Son extends Father {
	public int getSuperA() {
		return super.a;
	}

	public int getA() {
		return a;
	}
}

public class Test {
	public static void main(String args[]) {
		Son son = new Son();
		System.out.println(son.getA() + " " + son.getSuperA());
	}
}
